@{
    ViewBag.Title = "Delete Contact";
}

<table><tr><td height="10px"></td></tr></table>

<table cellpadding="4" cellspacing="4" width="70%" align="center" style="background-color: skyblue; border:solid 2px red; padding-top:20px;">
    <tr><td colspan="3" style="background-color:gray; font-size:18pt; font-weight:bold; height:30px; text-align:center;">Delete Contact Information</td></tr>
    <tr>

        <td style="text-align:right;">Contact ID</td>
        <td>{{Contact.contactID}}  </td>
    </tr>
    <tr>

        <td style="text-align:right;">First Name # </td>
        <td> {{Contact.firstName}}  </td>
    </tr>

    <tr>

        <td style="text-align:right;">Last Name # </td>
        <td> {{Contact.lastName}}  </td>
    </tr>
    <tr>
        <td style="text-align:right;">Email # </td>
        <td>{{Contact.email}}  </td>
    </tr>
    <tr>
        <td style="text-align:right;">Phone Number # </td>
        <td>{{Contact.phoneNumber}}  </td>
    </tr>
    <tr>
        <td style="text-align:right;">Status # </td>
        <td>{{contact.isActive?"Active":"InActive"}}  </td>
    </tr>
    
    <tr>
        <td></td>
        <td>
            <input type="button" value="Delete" ng-click="delete()" />
        </td>
    </tr>

</table>  